Value Filldown sysname (\S+)
Value Filldown model ([^:\b]+)
Value source_ip (\S+)
Value description (\S+)

Start
  ^.+ -oCheckHostIP -> Source
  ^.+ show chassis -> Chassis
  ^\s{4}filter -> Filter

Source
  ^.+@${source_ip}'s\s+password: -> Start

Chassis
  ^\s+Name\s+:\s${sysname}
  ^\s+Type\s+:\s${model} -> Start

Filter
  ^\s{8}ip-filter 2031 -> 2031
  ^\s{4}exit -> Start

2031
  ^\s{12}description "${description}"
  ^\s{12}entry 100 create -> Entry
  ^\s{8}exit -> Filter

Entry
  ^\s{16}action -> Action
  ^\s{12}exit -> 2031

Action
  ^\s{20}remark dscp "cs5" -> Record
  ^\s{16}exit -> Entry

EOF